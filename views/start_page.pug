doctype html
head
    title PKI_ZAL
    link(href='vendor/bootstrap/css/bootstrap.css' rel='stylesheet')
    link(href='css/one-page-wonder.css' rel='stylesheet')
    link(href='https://fonts.googleapis.com/css?family=Catamaran:100,200,300,400,500,600,700,800,900' rel='stylesheet')
    link(href='https://fonts.googleapis.com/css?family=Lato:100,100i,300,300i,400,400i,700,700i,900,900i' rel='stylesheet')
    script(src='vendor/jquery/jquery.min.js')
    script(src='vendor/bootstrap/js/bootstrap.bundle.min.js')
body
    nav.navbar.navbar-expand-lg.navbar-dark.navbar-custom.fixed-top.container
        #navbarResponsive.collapse.navbar-collapse
        ul.navbar-nav.ml-auto
                if username == ""
                    li.nav-item
                        a.nav-link(href='/login') Zaloguj
                else
                    li.nav-item
                        span.nav-link(href='#') #{username}
                    li.nav-item
                        a.nav-link(href='/logout') Wyloguj
    
    .container.row
        if queryResult.length != 0
            form(action='/view_page' method='GET')
                .form-row.col
                    label(for='selectTable') Wybierz tabele
                    select#selectTable.form-control(name='selectTable')
                        each val in queryResult
                            option(value= val) #{val}
                    button.btn.btn-primary(type='submit') Poka&zdot; zawarto&sacute;&cacute; tabeli



